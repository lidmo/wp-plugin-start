{
  "name": "lidmo/wp-plugin-start",
  "description": "Lídmo WP plugin start",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Lídmo",
      "email": "suporte@lidmo.com.br"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "lidmo/wp-foundation": "dev-develop"
  },
  "autoload": {
    "psr-4": {
      "LidmoPrefix\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lidmo\\WP\\Installer\\": "bin/Installer/"
    },
  },
  "require-dev": {
    "symfony/var-dumper": "^5.4",
    "wp-phpunit/wp-phpunit": "^6.1",
    "yoast/phpunit-polyfills": "^1.0",
    "roots/wordpress": "^6.1",
    "composer/composer": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "roots/wordpress-core-installer": true
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "Lidmo\\WP\\Installer\\PostCreateProject::changePrefix"
    ]
  }
}
